[options]
  UseSyslog

[SSH]
  sequence = {{ port_sequence | join(",") }}
  tcpflags = syn
  seq_timeout = {{ sequence_timeout }}
  start_command = /sbin/iptables -I INPUT 1 -s %IP% -p tcp {{ multiport }} --dport {{ secure_ports | join(",") }} -j ACCEPT
  cmd_timeout = {{ command_timeout }}
  stop_command = /sbin/iptables -D INPUT -s %IP% -p tcp {{ multiport }} --dport {{ secure_ports | join(",") }} -j ACCEPT
